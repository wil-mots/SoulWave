enable
  configure terminal
    hostname SoWa_A-Switch-3
    no ip domain-name lookup
    username klaas secret uioppoiu
    enable secret uiop
    ipv6 unicast-routing
    ip domain-name soulwave.boo
    crypto key generate rsa modulus 1024
    service password-encryption
    banner motd #
 __   __                            ___ 
/__` /  \ |  | |    |  |  /\  \  / |__  
.__/ \__/ \__/ |___ |/\| /~~\  \/  |___ 
                                        #
    
    line con 0
      password uiop
      login
      exit

    line vty 0 4
      login local
      transport input ssh
      exit

    vlan 11
      name Researchers
    vlan 12
      name Communicatie
    vlan 13
      name Staff
    vlan 14
      name IT_Personeel
    vlan 15
      name Techniekers
    vlan 16
      name Guests
    vlan 99
      name Unused
    vlan 100
      name Management
    vlan 500
      name Native
      exit

    interface vlan 100
      ip address 172.16.100.5 255.255.255.0
      ipv6 address fc00:0:0:ff::5/64
      no shutdown

    ip default-gateway 172.16.100.2

    interface range f0/1 - 4
      switchport mode trunk
      switchport trunk native vlan 500
      switchport trunk allowed vlan 11,12,13,14,15,16,100,500
      no shutdown
      exit
